<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dcatalina.home=target -Dvogon.keystore.file=target/vogonweb.keystore -Dvogon.keystore.pass=password -Djava.io.tmpdir=target -classpath %classpath org.zlogic.vogon.web.Application</exec.args>
                <exec.executable>java</exec.executable>
                <Env.VOGON_ALLOW_REGISTRATION>true</Env.VOGON_ALLOW_REGISTRATION>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
            </goals>
            <properties>
                <exec.args>-agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address} -Dcatalina.home=target -Dvogon.keystore.file=target/vogonweb.keystore -Dvogon.keystore.pass=password -Djava.io.tmpdir=target -classpath %classpath org.zlogic.vogon.web.Application</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <Env.VOGON_ALLOW_REGISTRATION>true</Env.VOGON_ALLOW_REGISTRATION>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dcatalina.home=target -Dvogon.keystore.file=target/vogonweb.keystore -Dvogon.keystore.pass=password -Djava.io.tmpdir=target -classpath %classpath org.zlogic.vogon.web.Application</exec.args>
                <exec.executable>java</exec.executable>
                <Env.VOGON_ALLOW_REGISTRATION>true</Env.VOGON_ALLOW_REGISTRATION>
            </properties>
        </action>
    </actions>
